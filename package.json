{
  "name": "seal-hub-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://hub.sealcred.xyz",
  "scripts": {
    "start": "yarn generate-css-types && vite",
    "build": "yarn generate-css-types && tsc && cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts",
    "preview": "yarn build && yarn vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.1.81",
    "@big-whale-labs/frontend-utils": "^0.1.2",
    "@big-whale-labs/seal-hub-contract": "^0.0.11",
    "@big-whale-labs/seal-hub-kit": "^0.0.33",
    "@big-whale-labs/stores": "^0.1.18",
    "@ethersproject/experimental": "^5.7.0",
    "@material-tailwind/react": "^1.4.2",
    "@opengsn/provider": "3.0.0-beta.3",
    "@rainbow-me/rainbowkit": "^0.12.1",
    "buffer": "^6.0.3",
    "comlink": "^4.4.1",
    "envalid": "^7.3.1",
    "ethers": "^5.7.2",
    "json-bigint": "^1.0.0",
    "ky": "^0.33.3",
    "preact": "^10.13.1",
    "valtio": "^1.7.6",
    "wagmi": "^0.12.2",
    "wouter": "^2.10.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@preact/preset-vite": "^2.5.0",
    "@rollup/plugin-inject": "^5.0.3",
    "@types/elliptic": "^6.4.14",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "assert-browserify": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.6.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react-scripts": "^5.0.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-classnames": "^3.0.7",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-comlink": "^3.0.5",
